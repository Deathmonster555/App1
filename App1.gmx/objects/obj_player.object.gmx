            <kind>1</kind>
            <string>//if unconnected, find the closest target (one you are touching)
if orbit=0
{
    connect = instance_nearest(x,y,obj_debris_parent)
}

//rotate the player towards an upright position
if phy_rotation &gt; 5 &amp;&amp; phy_rotation&lt;160
{
    physics_apply_torque(-20)
}

//rotate the player the other way to an upright position
if phy_rotation&gt;180 &amp;&amp; phy_rotation &lt; 355
{
    physics_apply_torque(20)
}

//reset angle of player to not cause rotation issues
 if phy_rotation&gt;360
 {
    phy_rotation = 0
 }
 if phy_rotation&lt;0
 {
 phy_rotation=360
 }
 
//gravity
physics_apply_force(x,y,0,800)

//set first position of slingshot
if mouse_check_button_pressed(mb_left)&amp;&amp;orbit!=0
{
    p1x=window_mouse_get_x()
    p1y=window_mouse_get_y()
    launch = 'calculating'
}

//launch the player and disconnect him from object
if mouse_check_button_released(mb_left)&amp;&amp;orbit!=0
{
    physics_joint_delete(orbit)
    check = 1
    alarm[0] = 1
    p2x=window_mouse_get_x()
    p2y=window_mouse_get_y()
    physics_apply_force(x,y,(150*p1x)-(150*p2x),(150*p1y)-(150*p2y))
    launch='not'
    audio_play_sound(snd_jump,1,0)
    orbit = 0
}

//restart when dead (highscore screen needed)
if phy_position_y&gt;view_yview[1]+view_hview[1]
{
    room_restart()
}

if y &lt; view_yview[1]{
view_visible[2] = 1
view_xview[2]=x-64
view_yview[2]=y-64
}else{
view_visible[2] = 0}

draw_set_alpha(256)

</string>
